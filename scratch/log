assert do_algebra(['-', '*', '+'], [2, 3, 4, 5]) == 9 # output: pop from empty list
assert do_algebra(['*', '+', '-'], [2, 3, 4, 5]) == 3 # output: pop from empty list
assert do_algebra(['+', '*', '*', '//'], [1, 2, 3, 4, 5]) == 3 # output: pop from empty list
assert do_algebra(['**', '-', '+'], [2, 3, 4, 5]) == 3 # output: pop from empty list
