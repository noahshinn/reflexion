    left = -100
    right = 100
    epsilon = 1e-6

    while abs(right - left) > epsilon:
        mid = (left + right) / 2
        if poly(xs, mid) == 0:
            return mid
        elif poly(xs, mid) * poly(xs, left) < 0:
            right = mid
        else:
            left = mid

